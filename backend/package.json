{
  "name": "digital-meeting-assistant-backend",
  "version": "1.0.0",
  "description": "Backend API for Digital Meeting Assistant",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "node src/scripts/runMigrations.js",
