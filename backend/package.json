    "db:sync": "node src/scripts/syncDatabase.js",
    "db:sync:force": "node src/scripts/syncDatabase.js --force",
    "db:setup": "npm run db:create && npm run db:sync",
    "test:registration": "node src/scripts/testRegistration.js",
    "test": "echo \"Error: no test specified\" && exit 1"
