    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "pdfkit": "^0.14.0",
    "qrcode": "^1.5.3",
    "sequelize": "^6.35.2",
    "socket.io": "^4.7.2",
